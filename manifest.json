{
    "manifest_version": 3,
    "name": "Pubspec auto view",
    "description": "Auto view Pubspec files in github files review",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://github.com/*/pull/*/files"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab"
    ]
}